{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rules": {
        "object-literal-sort-keys": true,
        "no-console": false,
        "no-empty-interface": true,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-snake-case"]
    }
}
